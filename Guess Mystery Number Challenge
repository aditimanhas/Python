import random

empty_list = []
for i in range(1, 101):
    empty_list.append(str(i))

empty_list = set(empty_list)
empty_list = list(empty_list)
random_num = int(empty_list[0])

print("ğŸ² Welcome to the Guessing Game! ğŸ²")
print("You have 5 chances to guess the correct number between 1 and 100. ğŸ¤")

for i in range(1, 5 + 1):
    value = int(input('Enter your guess: '))
    
    if value == random_num:
        print('ğŸ‰ğŸŠ You win! Congratulations! ğŸŠğŸ‰')
        break
    elif value < random_num:
        print('â¬†ï¸ Your guess is too low.')
    else:
        print('â¬‡ï¸ Your guess is too high.')
    
    print(f"ğŸ’¡ Attempts left: {5-i}\n")
else:
    print(f'ğŸ˜¢ Sorry, you lost! The correct number was {random_num}.')

